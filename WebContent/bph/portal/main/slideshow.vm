<div id="slider1_container">
  
  <div u="slides" class="child"> 
  	#foreach ( $slideShow in $listSlideShow )
    <div><img u="image" src="../download?file=$!uploadDir$!slideShow.fileName" /></div>
    #end 
  </div>
  
  <!-- bullet navigator container -->
  <div u="navigator" class="jssorb01" style="position: absolute; top: 305px; left: 50%; margin-left: -50px;">
    <!-- bullet navigator item prototype -->
    <div u="prototype" style="position: absolute; width: 12px; height: 12px;"></div>
  </div>
  
  <!-- Arrow Left -->
  <span u="arrowleft" class="jssora05l" style="width: 40px; height: 40px; top: 142px; left: 1px;"></span>
  <!-- Arrow Right -->
  <span u="arrowright" class="jssora05r" style="width: 40px; height: 40px; top: 142px; right: 1px"></span> 
  
</div>
