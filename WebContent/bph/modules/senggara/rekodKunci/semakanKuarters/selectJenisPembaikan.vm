<select class="texts" id="idJenisPembaikan" name="idJenisPembaikan" style="width:100%" onChange="doDivAjaxCall3$formname('divSemakanKuarters','doChangeJenisPembaikan','')">
    <option value="">SILA PILIH</option>
    <option value="1" #if ($!idJenisPembaikan == '1') selected #end >TIADA PEMBAIKAN / SEDIA DIDUDUKI</option>
    <option value="2" #if ($!idJenisPembaikan == '2') selected #end >PEMBAIKAN MELALUI INDEN KERJA</option>
    <option value="3" #if ($!idJenisPembaikan == '3') selected #end >PEMBAIKAN OLEH AGENSI PELAKSANA / PEMAJU</option>
</select>
