<div id="content-format"><pinLeft></pinLeft><pinRight></pinRight>
  <header>SEMAKAN ADUAN</header>
  <fieldset>
      <section>
		<table cellpadding="10" cellspacing="0" border="0" width="100%" style="border: 1px dotted #CCC; padding: 15px" align="center">
			<tr>
    			<td width="75px"></td>
    			<td width="5px"></td>
    			<td></td>
    		</tr>
    		<tr>
    			<td nowrap>No Kad Pengenalan</td>
    			<td>:</td>
    			<td>
    				<input class="input-style" id="noKadPengenalan" name="noKadPengenalan" type="text" size="30" onBlur="this.value=this.value.toUpperCase();"/>
    				<div id="err_noKadPengenalan" style="color:#CC0000;font-weight:bold;border:2px #000"></div>
    			</td>
    		</tr>
    		<tr>
    			<td>No Aduan</td>
    			<td>:</td>
    			<td>
    				<input class="input-style" id="noAduan" name="noAduan" type="text" size="30" onBlur="this.value=this.value.toUpperCase();"/>
    				<div id="err_noAduan" style="color:#CC0000;font-weight:bold;border:2px #000"></div>
    			</td>
    		</tr>
    		<tr>
    			<td colspan="3">
    				<table align="center">
    					<tr>
    						<td><input class="button-style" id="btnHantarSemakAduan" type="button" value="Hantar"></td>
    					</tr>
    				</table>
    			</td>
    		</tr>
		</table>
      </section>
  </fieldset>
</div>