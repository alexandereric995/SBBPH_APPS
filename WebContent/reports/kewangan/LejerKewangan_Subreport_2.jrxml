	<queryString><![CDATA[SELECT month(kew_bayaran_resit.tarikh_bayaran),
       kew_bayaran_resit.tarikh_bayaran,
       kew_invois.kredit,
       kew_invois.debit,
       kew_invois.keterangan_bayaran,
       kew_bayaran_resit.no_resit,
       kew_bayaran_resit.id,
       kew_invois.id_pembayar,
       kew_invois.id_jenis_bayaran       
  FROM    (   sbbphdb.kew_resit_senarai_invois kew_resit_senarai_invois
           JOIN
              sbbphdb.kew_bayaran_resit kew_bayaran_resit
           ON (kew_resit_senarai_invois.id_bayaran_resit =
                  kew_bayaran_resit.id))
       JOIN
          sbbphdb.kew_invois kew_invois
       ON (kew_invois.id = kew_resit_senarai_invois.id_invois)
WHERE kew_invois.id_pembayar = $P{userId} AND kew_invois.id_jenis_bayaran = $P{jenisBayaran}]]></queryString>

