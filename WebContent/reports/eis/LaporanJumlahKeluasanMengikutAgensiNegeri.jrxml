ruj_bandar.keterangan as Bandar, ruj_negeri.keterangan as Negeri, ruj_status.keterangan as Status
from bgs_permohonan as a
join ruj_agensi on a.id_agensi = ruj_agensi.id
join dev_bangunan on a.id_bangunan = dev_bangunan.id
join dev_premis on dev_bangunan.id_premis = dev_premis.id
join ruj_bandar on ruj_bandar.id = a.id_bandar
join ruj_negeri on ruj_bandar.id_negeri = ruj_negeri.id
join ruj_status on a.id_status = ruj_status.id
$P!{negeri_1}
$P!{agensi_1}]]></queryString>

