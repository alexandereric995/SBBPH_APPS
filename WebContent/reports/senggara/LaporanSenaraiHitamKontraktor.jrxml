	<parameter name="tarikh_hingga" isForPrompting="true" class="java.lang.String"/>
	<parameter name="ReportDir" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT mtn_kontraktor.flag_awam,
       mtn_kontraktor.flag_elektrik,
       ruj_kawasan.keterangan kawasan,
       mtn_kontraktor.no_pendaftaran,
       mtn_kontraktor.nama_kontraktor,
       mtn_kontraktor.nama_pemilik,
       mtn_kontraktor.no_telefon,
       mtn_kontraktor.no_telefon_bimbit,
       mtn_kontraktor.alamat1,
       mtn_kontraktor.alamat2,
       mtn_kontraktor.alamat3,
       mtn_kontraktor.poskod,
       ruj_bandar.keterangan bandar,
       ruj_negeri.keterangan negeri,
       mtn_kontraktor_senarai_hitam.tarikh_mula,
       mtn_kontraktor_senarai_hitam.tarikh_tamat,
       mtn_kontraktor_senarai_hitam.sebab,
       mtn_kontraktor_senarai_hitam.catatan
  FROM    (   (   (   mtn_kontraktor
                   JOIN
                      ruj_bandar
                   ON (mtn_kontraktor.id_bandar = ruj_bandar.id))
               JOIN
                  mtn_kontraktor_senarai_hitam
               ON (mtn_kontraktor_senarai_hitam.no_pendaftaran =
                      mtn_kontraktor.no_pendaftaran))
           JOIN
              ruj_kawasan
           ON (mtn_kontraktor.id_kawasan = ruj_kawasan.id))
       JOIN
          ruj_negeri
       ON (ruj_bandar.id_negeri = ruj_negeri.id)
 WHERE mtn_kontraktor_senarai_hitam.tarikh_mula 
   BETWEEN str_to_date ('$P!{tarikh_mula}','%d-%m-%Y')
     AND str_to_date ('$P!{tarikh_hingga}','%d-%m-%Y')
ORDER BY ruj_bandar.keterangan,ruj_kawasan.keterangan ASC]]></queryString>

