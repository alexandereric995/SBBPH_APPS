	<parameter name="kodPusat" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT  MONTH(A.tarikh_bayaran), YEAR(A.tarikh_bayaran), A.no_resit, A.flag_void, A.tarikh_bayaran, SUM(A.jumlah_amaun_bayaran) jumlah_amaun_bayaran
FROM kew_bayaran_resit A
JOIN ruj_kod_juruwang F ON A.id_juruwang = F.id
WHERE MONTH(A.tarikh_bayaran) = '$P!{bulan}'
AND YEAR(A.tarikh_bayaran) = '$P!{tahun}'
AND F.kod_pusat_terima = '$P!{kodPusat}'
AND A.flag_jenis_bayaran != 'ONLINE'
AND A.id_juruwang is not null
GROUP BY MONTH(A.tarikh_bayaran), YEAR(A.tarikh_bayaran), A.no_resit, A.flag_void, A.tarikh_bayaran
ORDER BY A.tarikh_bayaran, A.no_resit]]></queryString>

