	<queryString><![CDATA[SELECT mtn_kuarters.jenis_laporan, mtn_kuarters.tarikh_terima_laporan, ruj_lokasi_kuarters.keterangan lokasikuarters,
ruj_lokasi_permohonan.lokasi, ruj_negeri.keterangan, kua_kuarters.no_rujukan,
kua_kuarters.no_unit, kua_kuarters.blok, kua_kuarters.alamat_1, kua_kuarters.alamat_2,
mtn_kuarters.keterangan_laporan, kua_kuarters.id_kelas_kuarters, ruj_status.keterangan status
FROM ((((( sbbphdb.ruj_lokasi_permohonan ruj_lokasi_permohonan
JOIN  sbbphdb.ruj_negeri ruj_negeri
 ON (ruj_lokasi_permohonan.id_negeri = ruj_negeri.id))
JOIN  sbbphdb.ruj_lokasi_kuarters ruj_lokasi_kuarters
 ON (ruj_lokasi_kuarters.id_lokasi_permohonan = ruj_lokasi_permohonan.id))
JOIN  sbbphdb.kua_kuarters kua_kuarters
 ON (kua_kuarters.id_lokasi_kuarters = ruj_lokasi_kuarters.id))
JOIN  sbbphdb.mtn_kuarters mtn_kuarters
 ON (mtn_kuarters.id_kuarters = kua_kuarters.id))
JOIN  sbbphdb.ruj_status ruj_status
 ON (mtn_kuarters.status = ruj_status.id))
JOIN  sbbphdb.ruj_kelas_kuarters ruj_kelas_kuarters
 ON (kua_kuarters.id_kelas_kuarters = ruj_kelas_kuarters.id)]]></queryString>

