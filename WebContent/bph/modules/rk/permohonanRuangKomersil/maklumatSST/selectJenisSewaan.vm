<select id="idJenisSewa" name="idJenisSewa" style="width:100%" onChange="$('err_idJenisSewa').innerHTML=''; at(this, event);doDivAjaxCall$formname('divTab','doChangeMaklumatSST','');">
    <option value="">SILA PILIH</option>
    <option #if ( "H" ==  $!sst.idJenisSewa ) selected #end value="H">HARIAN</option>
    <option #if ( "B" ==  $!sst.idJenisSewa ) selected #end value="B">BULANAN</option>
    <option #if ( "T" ==  $!sst.idJenisSewa ) selected #end value="T">TAHUNAN</option>
</select>
