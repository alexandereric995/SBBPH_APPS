FROM ( (rpp_permohonan b
LEFT JOIN ruj_status ON (b.id_status = ruj_status.id))
LEFT JOIN rpp_peranginan c ON (b.id_peranginan = c.id))
LEFT JOIN users a ON (b.id_pemohon = a.user_login)
