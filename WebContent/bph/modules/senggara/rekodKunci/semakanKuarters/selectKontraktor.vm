<select id="idKontraktor" name="idKontraktor" style="width:100%" onChange="$('err_idKontraktor').innerHTML=''; at(this, event);">
  <option value="">SILA PILIH</option>
  <option #if ($!idKontraktor == 'JKR') selected #end value="JKR">JKR</option>
  <option #if ($!idKontraktor == 'PJH') selected #end value="PJH">PJH</option>
  <option #if ($!idKontraktor == 'PPJ') selected #end value="PPJ">PPj</option>
  <option #if ($!idKontraktor == 'L') selected #end value="L">LAIN - LAIN</option>		        
		      
</select>
