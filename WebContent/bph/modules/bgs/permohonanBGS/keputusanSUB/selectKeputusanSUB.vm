<select id="idKeputusan" name="idKeputusan" style="width:100%" onChange="$('err_idKeputusan').innerHTML=''; at(this, event);">
  <option value="">SILA PILIH</option>
  <option #if ($!r.flagKeputusan == 'L') selected #end value="L">LULUS</option>
  <option #if ($!r.flagKeputusan == 'T') selected #end value="T">TOLAK</option>
</select>
