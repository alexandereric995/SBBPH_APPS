	</parameter>
	<queryString><![CDATA[SELECT kua_temujanji.tarikh_mula_temujanji, users.no_kp, users.user_name
  FROM kua_agihan kua_agihan
        INNER JOIN users users
           ON (kua_agihan.id_pemohon = users.user_login)
       INNER JOIN kua_temujanji kua_temujanji
          ON (kua_temujanji.id_agihan = kua_agihan.id)
  	WHERE kua_temujanji.id is not null
$P!{findNoKP_cond}
$P!{findDateTarikhTemujanji_cond}]]></queryString>

