	<queryString><![CDATA[SELECT rpp_maklumbalas.no_maklumbalas,
       rpp_maklumbalas.keterangan maklumbalas,
       rpp_maklumbalas.tarikh_maklumbalas,
       rpp_maklumbalas.ulasan_maklumbalas
FROM ( rpp_permohonan
  JOIN
    rpp_maklumbalas
     ON (rpp_permohonan.id_pemohon = rpp_maklumbalas.id_permohonan))
  JOIN
    users
     ON (users.user_login = rpp_permohonan.id_pemohon)
WHERE rpp_maklumbalas.tarikh_maklumbalas
   BETWEEN str_to_date ('$P!{tarikh_mula}','%d-%m-%Y')
     AND str_to_date ('$P!{tarikh_hingga}','%d-%m-%Y')]]></queryString>

