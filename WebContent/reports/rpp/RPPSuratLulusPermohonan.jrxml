	<queryString><![CDATA[SELECT users.user_login, users.user_name, users.no_telefon, users.no_faks,
users.user_address, users.user_address2, users.user_address3,
users.user_postcode, users.user_bandar,
int_hrmis.no_pengenalan_pasangan no_kp_pasangan, int_hrmis.nama_pasangan,
rpp_permohonan.id_peranginan, rpp_peranginan.nama_peranginan, rpp_peranginan.id_jenis_peranginan,
rpp_peranginan.alamat_1, rpp_peranginan.alamat_2, rpp_peranginan.alamat_3, rpp_peranginan.poskod,
ruj_bandar.keterangan bandarperanginan, ruj_negeri.keterangan negeriperanginan,
rpp_permohonan.no_tempahan, rpp_permohonan.bil_unit, rpp_permohonan.bil_dewasa,
rpp_permohonan.bil_tambahan_dewasa, rpp_permohonan.bil_kanak_kanak,
rpp_permohonan.id_jenis_unit_rpp, rpp_permohonan.tarikh_masuk_rpp, rpp_permohonan.tarikh_keluar_rpp,
DATEDIFF(rpp_permohonan.tarikh_keluar_rpp,rpp_permohonan.tarikh_masuk_rpp) AS totalMalam, rpp_permohonan.jumlah_amaun,
ruj_jenis_unit_rpp.keterangan jenisunit, ruj_jenis_unit_rpp.kadar_sewa, ruj_jenis_unit_rpp.keterangan,
rpp_akaun.amaun_bayaran_seunit, CASE WHEN rpp_akaun.id_kod_hasil = "72311" then rpp_akaun.debit else "" end deposit, rpp_akaun.id_kod_hasil,
SUM(rpp_akaun.debit) AS JUMLAH
FROM  users
LEFT JOIN  users_spouse ON users.user_login = users_spouse.user_login
LEFT JOIN  rpp_permohonan ON rpp_permohonan.id_pemohon = users.user_login
LEFT JOIN  rpp_peranginan ON rpp_permohonan.id_peranginan = rpp_peranginan.id
LEFT JOIN  ruj_jenis_unit_rpp ON rpp_permohonan.id_jenis_unit_rpp = ruj_jenis_unit_rpp.id
LEFT OUTER JOIN  rpp_akaun ON rpp_permohonan.id = rpp_akaun.id_permohonan
LEFT JOIN  ruj_bandar ON rpp_peranginan.id_bandar = ruj_bandar.id
LEFT JOIN  ruj_negeri ON ruj_bandar.id_negeri = ruj_negeri.id
LEFT OUTER JOIN int_hrmis ON int_hrmis.no_pengenalan = users.no_kp
WHERE rpp_permohonan.id = '$P!{idRppPermohonan}']]></queryString>

