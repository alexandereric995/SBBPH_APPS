	<queryString><![CDATA[SELECT kew_penyata_pemungut.no_penyata_pemungut, kew_penyata_pemungut.tarikh_penyata_pemungut,
kew_bayaran_resit.jumlah_amaun_bayaran, kew_bayaran_resit.tarikh_bayaran, ruj_cara_bayar.keterangan jenisBayaran
FROM    (    kew_penyata_pemungut
JOIN ruj_cara_bayar
 ON (kew_penyata_pemungut.id_kaedah_bayaran = ruj_cara_bayar.id))
JOIN kew_bayaran_resit
 ON (kew_bayaran_resit.tarikh_bayaran = kew_penyata_pemungut.tarikh_kutipan_dari)
WHERE MONTH(kew_bayaran_resit.tarikh_bayaran) = '$P!{bulan}'
AND YEAR(kew_bayaran_resit.tarikh_bayaran) = '$P!{tahun}'
GROUP BY kew_penyata_pemungut.no_penyata_pemungut, kew_bayaran_resit.jumlah_amaun_bayaran]]></queryString>

