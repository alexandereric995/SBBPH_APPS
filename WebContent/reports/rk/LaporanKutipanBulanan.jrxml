	<parameter name="ReportDir" isForPrompting="true" class="java.lang.String"/>
	<parameter name="TAHUN" isForPrompting="true" class="java.lang.String"/>
	<parameter name="ID_SEKSYEN" isForPrompting="true" class="java.lang.String"/>
	<parameter name="ID_SEKSYEN_1" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[(!$P{ID_SEKSYEN}.trim().equals("") ? "AND rk_ruang_komersil.id_seksyen = '" + $P{ID_SEKSYEN} + "'" : "")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT ruj_bulan.keterangan,
       (SELECT ifnull(sum(ifnull(rk_akaun.debit, 0)), 0)
        FROM rk_akaun, rk_fail, rk_ruang_komersil
        WHERE     rk_akaun.id_fail = rk_fail.id
              AND rk_fail.id_ruang = rk_ruang_komersil.id
              AND rk_akaun.id_kod_hasil = 74202
              AND rk_akaun.id_jenis_transaksi IN (1)
              AND DATE_FORMAT(rk_akaun.tarikh_invois, '%m') = (ruj_bulan.kod_bulan + 1)
              AND DATE_FORMAT(rk_akaun.tarikh_invois, '%Y') = '$P!{TAHUN}'
              $P!{ID_SEKSYEN_1})
          AS total_patut_bayar,
       (SELECT ifnull(sum(ifnull(rk_akaun.kredit, 0)), 0)
        FROM rk_akaun, rk_fail, rk_ruang_komersil
        WHERE     rk_akaun.id_fail = rk_fail.id
              AND rk_fail.id_ruang = rk_ruang_komersil.id
              AND rk_akaun.id_kod_hasil = 74202
              AND rk_akaun.id_jenis_transaksi IN (2, 4)
              AND DATE_FORMAT(rk_akaun.tarikh_transaksi, '%m') = (ruj_bulan.kod_bulan + 1)
              AND DATE_FORMAT(rk_akaun.tarikh_transaksi, '%Y') = '$P!{TAHUN}'
              $P!{ID_SEKSYEN_1})
          AS total_bayaran
FROM ruj_bulan
ORDER BY ruj_bulan.kod_bulan ASC]]></queryString>

