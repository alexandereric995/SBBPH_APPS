	</parameter>
	<queryString><![CDATA[SELECT kontrak_bon.id_kontrak, kontrak_bon.status_bon, kontrak_bon.no_rujukan, kontrak_bon.nilai_bon, kontrak_bon.tarikh_luput, 
kontrak_kontraktor.nama_kontraktor, kontrak_kontrak.tempoh, kontrak_kontrak.nilai_kontrak,
ruj_jenis_jaminan.keterangan jenis_jaminan, year (kontrak_bon.tarikh_luput), ruj_lantikan_kontrak.keterangan LantikanOleh,
ruj_seksyen.keterangan Seksyen, ruj_jenis_kontrak.keterangan JenisKontrak
FROM kontrak_bon
LEFT OUTER JOIN kontrak_kontrak ON (kontrak_bon.id_kontrak = kontrak_kontrak.id)
LEFT OUTER JOIN kontrak_kontraktor ON (kontrak_kontrak.id = kontrak_kontraktor.no_pendaftaran)
LEFT OUTER JOIN ruj_jenis_jaminan ON (kontrak_bon.id_jenis_jaminan = ruj_jenis_jaminan.id)
LEFT OUTER JOIN ruj_lantikan_kontrak ON (kontrak_kontrak.id_pelaksana = ruj_lantikan_kontrak.id)
LEFT OUTER JOIN ruj_seksyen ON (kontrak_kontrak.id_seksyen = ruj_seksyen.id)
LEFT OUTER JOIN ruj_jenis_kontrak ON (kontrak_kontrak.id_jenis_kontrak = ruj_jenis_kontrak.id)
WHERE kontrak_bon.id is not null
$P!{tarikh_1}
$P!{idSeksyen_1}
$P!{idJenisKontrak_1}
$P!{idLantikanKontrak_1}]]></queryString>

