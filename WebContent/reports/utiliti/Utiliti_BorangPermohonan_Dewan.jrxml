	<parameter name="idPermohonan" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT users.user_name,
       users.no_kp,
       users.alamat_1,
       users.alamat_2,
       users.alamat_3,
       users.poskod,
       ruj_bandar.keterangan bandar,
       ruj_negeri.keterangan negeri,
       ruj_agensi.keterangan agensi,
       ruj_kementerian.keterangan kementerian,
       users.emel,
       users.no_telefon_bimbit,
       users.no_telefon,
       util_dewan.nama dewan,
       util_permohonan.tujuan,
       util_permohonan.tarikh_mula,
       util_permohonan.tarikh_tamat
FROM   ((((( users
     JOIN
       ruj_agensi
         ON (users.id_agensi = ruj_agensi.id))
     JOIN
       util_permohonan
         ON (util_permohonan.id_pemohon = users.user_login))
     JOIN
       util_dewan
         ON (util_permohonan.id_dewan = util_dewan.id))
     JOIN
       ruj_bandar
         ON (users.id_bandar = ruj_bandar.id))
     JOIN
       ruj_negeri
         ON (ruj_bandar.id_negeri = ruj_negeri.id))
     JOIN
       ruj_kementerian
         ON (ruj_agensi.id_kementerian = ruj_kementerian.id)
WHERE util_permohonan.id= $P{idPermohonan}
]]></queryString>

