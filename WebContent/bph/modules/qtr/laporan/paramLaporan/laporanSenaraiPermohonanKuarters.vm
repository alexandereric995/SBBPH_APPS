<fieldset>
<legend class="legend">SENARAI PERMOHONAN KUARTERS</legend>
<table width="100%">
  <tr>
    <td style="width:150px"></td>
    <td style="width:10px"></td>
    <td></td>
  </tr>
  <tr>
                    <td class="label" valign="top">Tarikh Terima&nbsp;<span style="color:#FF2A00">*</span></td>
                    	<td class="label" valign="top">:</td>
                    	<td valign="top">
                    		<div>
                        		<input type="text" id="tarikhTerima" name="tarikhTerima" value="" >
                        		<span style="color:#FF2A00">&nbsp;cth: dd-mm-yyyy</span>
                    			<script>
                                	_jq("#tarikhTerima").Zebra_DatePicker({ 
                        				format: 'd-m-Y',
                    					readonly_element: false
                                	});
                    				_jq(".Zebra_DatePicker").css("zIndex", 2147483647);
                        		</script>
                        		<div id="err_tarikhTerima" style="color:#CC0000;font-weight:bold;border:2px #000"></div>
                    		</div>
                    	</td>
                    </tr>
					<tr>
                    	<td class="label" valign="top">Tarikh Terima Hingga&nbsp;<span style="color:#FF2A00">*</span></td>
                    	<td class="label" valign="top">:</td>
                    	<td valign="top">
                    		<div>
                        		<input type="text" id="tarikhTerimaHingga" name="tarikhTerimaHingga" value="" >
                        		<span style="color:#FF2A00">&nbsp;cth: dd-mm-yyyy</span>
                    			<script>
                                	_jq("#tarikhTerimaHingga").Zebra_DatePicker({ 
                        				format: 'd-m-Y',
                    					readonly_element: false
                                	});
                    				_jq(".Zebra_DatePicker").css("zIndex", 2147483647);
                        		</script>
                        		<div id="err_tarikhTerimaHingga" style="color:#CC0000;font-weight:bold;border:2px #000"></div>
                    		</div>
                    	</td>
                    </tr>
  <tr>
	<td></td>
	<td></td>
    <td>
    <input type="button" id="btnPrintLaporan" name="btnPrintLaporan" value="Cetak Laporan" onclick="doCetakLaporanSenaraiPermohonanKuarters()" />
    </td>
  </tr>
</table>
</fieldset>

<script>
  _jq(function($) {
      $('.currency').autoNumeric('init'); 
 	  $('.number').autoNumeric('init', {aSep: '', vMin: '0', vMax: '999999999999999', lZero: 'keep'} );
 	  $('.postcode').autoNumeric('init', {aSep: '', vMin: '00000', vMax: '99999', lZero: 'keep'} );
 	  $('.mykad').autoNumeric('init', {aSep: '', vMin: '000000000000', vMax: '999999999999', lZero: 'allow'} );
  });
</script>