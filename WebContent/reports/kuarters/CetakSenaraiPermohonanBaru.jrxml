	</parameter>
	<queryString><![CDATA[SELECT kua_permohonan.no_permohonan,
       users.user_name,
       users.no_kp,
       kua_permohonan.tarikh_permohonan,
       kua_permohonan.tarikh_kemaskini,
       ruj_lokasi_permohonan.lokasi,
       ruj_status.keterangan
FROM kua_permohonan kua_permohonan
LEFT JOIN ruj_status ruj_status ON (kua_permohonan.status = ruj_status.id)
LEFT JOIN users users ON (kua_permohonan.id_pemohon = users.user_login)
LEFT JOIN ruj_lokasi_permohonan ruj_lokasi_permohonan ON (kua_permohonan.id_lokasi_permohonan = ruj_lokasi_permohonan.id)
WHERE kua_permohonan.id is not null
$P!{findNoPermohonan_cond}
$P!{findNamaPemohon_cond}
$P!{findNoKPPemohon_cond}
$P!{findLokasiPermohonan_cond}
$P!{findTarikhPermohonan_cond}]]></queryString>

