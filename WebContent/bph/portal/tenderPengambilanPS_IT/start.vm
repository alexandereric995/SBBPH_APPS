<script>
    _jq(function() {
    	_jq( "#tabs" ).tabs({
			show: { effect: "fade", duration: 1500 }
		});
    });
	
	_jq('#contentPKios').hide();
	_jq('#contentPKafe').hide();
	
	_jq('#btnPHouseKeeping').click(function() {
		_jq('#contentPHouseKeeping').slideDown('slow');
		_jq('#contentPKafe').fadeOut();
		_jq('#contentPKios').fadeOut();
	});
	
	_jq('#btnPKafe').click(function() {
		_jq('#contentPHouseKeeping').slideUp();
		_jq('#contentPKafe').fadeIn('slow');
		_jq('#contentPKios').slideUp();
	});
	
	_jq('#btnPKios').click(function() {
		_jq('#contentPHouseKeeping').slideUp();
		_jq('#contentPKafe').slideUp();
		_jq('#contentPKios').fadeIn('slow');
	});
</script>

