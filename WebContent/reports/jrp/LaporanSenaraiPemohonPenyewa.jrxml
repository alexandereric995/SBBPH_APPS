	</parameter>
	<queryString><![CDATA[SELECT jrp_permohonan.no_permohonan,
       jrp_permohonan.tarikh_terima,
       jrp_permohonan.id_jenis_permohonan,
       ruj_agensi.keterangan agensi,
       ruj_kementerian.keterangan kementerian,
       jrp_permohonan.alamat_1,
       jrp_permohonan.alamat_2,
       jrp_permohonan.alamat_3,
       jrp_permohonan.poskod,
       ruj_bandar.keterangan bandar,
       ruj_negeri.keterangan negeri,
       ruj_jenis_permohonan_jrp.keterangan jenis_permohonan,
       jrp_permohonan.kadar_sewa,
       ruj_status.keterangan status
FROM ((( jrp_permohonan jrp_permohonan
  JOIN
     ruj_bandar ruj_bandar
       ON (jrp_permohonan.id_bandar = ruj_bandar.id))
  JOIN
      ruj_agensi ruj_agensi
       ON (jrp_permohonan.id_agensi = ruj_agensi.id))
  JOIN
      ruj_kementerian ruj_kementerian
       ON (ruj_agensi.id_kementerian = ruj_kementerian.id))
  JOIN
      ruj_negeri ruj_negeri
       ON (ruj_bandar.id_negeri = ruj_negeri.id)
  JOIN
      ruj_jenis_permohonan_jrp ruj_jenis_permohonan_jrp
       ON (jrp_permohonan.id_jenis_permohonan = ruj_jenis_permohonan_jrp.id)
  JOIN
      ruj_status ruj_status
       ON (jrp_permohonan.id_status = ruj_status.id)
WHERE jrp_permohonan.id is not null
$P!{idNegeri_1}
$P!{idBandar_1}
$P!{idKementerian_1}
$P!{idAgensi_1}
$P!{idStatusPermohonanJRP_1}
$P!{idNoPermohonan_1}]]></queryString>

