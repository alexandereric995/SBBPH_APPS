LEFT OUTER JOIN ruj_agensi c ON (a.id_agensi = c.id)
LEFT OUTER JOIN ruj_bandar d ON (a.id_bandar = d.id)
LEFT OUTER JOIN ruj_negeri e ON (d.id_negeri = e.id)
