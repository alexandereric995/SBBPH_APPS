<fieldset>
<legend class="legend">Laporan Maklumat Pemohon</legend>
<table width="80%" cellpadding="1" cellspacing="2">
  <tr>
    <td class="label" style="width:15%"></td>
    <td style="width:1%"></td>
    <td style="width:70%"></td>
  </tr>
  <tr>
    <td class="label">No. Permohonan</td>
    <td>:</td>
    <td><input type="text" name="findNoPermohonan"></td>
  </tr>
  </tr> 
  
  <tr>
    <td class="label">Negeri</td>
    <td>:</td>
    <td> #parse("$path/findNegeri.vm") </td>
  </tr>
  <tr>
    <td class="label">Bandar</td>
    <td>:</td>
    <td><div id="findBandar"> #parse("$path/findBandar.vm") </div></td>
  </tr>
  <tr>
    <td class="label">Kementerian</td>
    <td>:</td>
    <td> #parse("$path/findKementerian.vm") </td>
  </tr>
  <tr>
    <td class="label">Agensi</td>
    <td>:</td>
    <td><div id="findAgensi"> #parse("$path/findAgensi.vm") </div></td>
  </tr>
  <tr>
    <td class="label">Status Permohonan</td>
    <td>:</td>
    <td> #parse("$path/findStatusPermohonan.vm") </td>
  </tr>
  <tr>
	<td></td>
	<td class="label"></td>
	<td><input type="button" name="cmdCetak" id="cmdCetak" value="Jana Laporan" onclick="javascript:doCetakLaporanMaklumatPemohon()"/><input type="button" name="cmdKembali" id="cmdKembali" value="Kembali" onclick="javascript:doKembali()"/></td>
</tr>
</table>
</fieldset>
