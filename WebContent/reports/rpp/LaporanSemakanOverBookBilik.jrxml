	<parameter name="tarikhMula" isForPrompting="true" class="java.lang.String"/>
	<parameter name="tarikhHingga" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT B.nama_peranginan,e.keterangan AS JENIS_UNIT, COUNT(*)BIL_TEMPAHAN, F.BIL_UNIT_DITAWAR
FROM RPP_PERMOHONAN A, RPP_PERANGINAN B, USERS C, RUJ_STATUS D, ruj_jenis_unit_rpp e,
(SELECT B.ID, C.NAMA_PERANGINAN, B.KETERANGAN AS JENIS_UNIT, A.STATUS , COUNT(*) BIL_UNIT_DITAWAR
FROM RPP_UNIT A, ruj_jenis_unit_rpp B, RPP_PERANGINAN C
WHERE A.ID_JENIS_UNIT= B.ID
AND B.ID_PERANGINAN = C.ID
AND A.STATUS='SALE'
GROUP BY B.ID_PERANGINAN, C.NAMA_PERANGINAN, A.ID_JENIS_UNIT,B.KETERANGAN ,A.STATUS
) F
WHERE A.tarikh_masuk_rpp >= STR_TO_DATE('$P!{tarikhMula}','%d-%m-%Y')
AND A.tarikh_masuk_rpp <= STR_TO_DATE('$P!{tarikhHingga}','%d-%m-%Y')
AND A.ID_PERANGINAN =B.ID
AND A.ID_PEMOHON = C.USER_LOGIN
#AND B.ID='3'
#and a.jenis_pemohon='KELOMPOK'
AND A.ID_PEMOHON NOT IN ('anon','faizal','740219015244')
AND D.ID in ('1425259713412','1435512646303','1425259713415')
AND A.ID_STATUS= D.ID
and A.id_jenis_unit_rpp =e.id
AND A.id_jenis_unit_rpp=F.ID
GROUP BY B.nama_peranginan,e.keterangan
ORDER BY B.ID]]></queryString>

