	<parameter name="findKelasKuarters_cond" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[(!$P{findKelasKuarters}.trim().equals("")  ? " AND ruj_kelas_kuarters.id ='" + $P{findKelasKuarters}+"'" : "")]]></defaultValueExpression>
	</parameter>
	<parameter name="findLokasiKuarters" isForPrompting="true" class="java.lang.String"/>
	<parameter name="findLokasiKuarters_cond" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[(!$P{findLokasiKuarters}.trim().equals("")  ? " AND ruj_lokasi_permohonan.id ='" + $P{findLokasiKuarters}+"'" : "")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT users.user_name nama,
       users.no_kp kadPengenalan,
       ruj_kelas_kuarters.id kelas,
       ruj_lokasi_permohonan.lokasi lokasi,
       kua_permohonan.tarikh_permohonan tarikhDaftar,
       kua_agihan.tarikh_kemaskini tarikhKemaskini,
       ruj_sebab_penolakan.keterangan sebabPenolakan
FROM kua_agihan kua_agihan
LEFT JOIN ruj_sebab_penolakan ruj_sebab_penolakan ON (kua_agihan.sebab_tolak = ruj_sebab_penolakan.id)
LEFT JOIN users users ON (kua_agihan.id_pemohon = users.user_login)
LEFT JOIN kua_permohonan kua_permohonan ON (kua_agihan.id_permohonan = kua_permohonan.id) AND (kua_permohonan.id_pemohon = users.user_login)
LEFT JOIN kua_kuarters kua_kuarters ON (kua_agihan.id_kuarters = kua_kuarters.id)
LEFT JOIN ruj_kelas_kuarters ruj_kelas_kuarters ON (kua_agihan.kelas_kuarters = ruj_kelas_kuarters.id)
LEFT JOIN ruj_lokasi_permohonan ruj_lokasi_permohonan ON (kua_permohonan.id_lokasi_permohonan = ruj_lokasi_permohonan.id)
WHERE kua_agihan.id is not null 
AND kua_agihan.status = '1432614959825'
$P!{findNoPermohonan_cond}
$P!{findNamaPemohon_cond}
$P!{findNoKPPemohon_cond}
$P!{findKelasKuarters_cond}
$P!{findLokasiKuarters_cond}]]></queryString>

