	<parameter name="tarikhHingga" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT a.no_resit,
       a.tarikh_bayaran,
       a.id_permohonan sellerOrderNo,
       a.id_transaksi_bank,
       a.jumlah_amaun_bayaran,
       c.user_name,
       c.user_login no_kp,
       a.masa_resit
FROM kew_bayaran_resit a, kew_resit_kaedah_bayaran b, users c
WHERE     a.id = b.id_bayaran_resit
      AND a.id_pembayar = c.user_login
      AND b.id_mod_bayaran IN ('FPX', 'MIGS')
      AND a.tarikh_bayaran BETWEEN str_to_date('$P!{tarikhMula}', '%d-%m-%Y')
                               AND str_to_date('$P!{tarikhHingga}',
                                               '%d-%m-%Y')
GROUP BY a.no_resit,
         a.tarikh_bayaran,
         a.id_permohonan,
         c.user_login
ORDER BY abs(a.no_resit), a.tarikh_bayaran]]></queryString>

