<select id="flagAktifSubInformasi" name="flagAktifSubInformasi" style="width:180px" onChange="$('err_flagAktifSubInformasi').innerHTML=''; at(this, event);">
	<option value="">SILA PILIH</option>
    <option value="Y" #if ($!subInformasi.flagAktif == 'Y') selected #end>AKTIF</option>
    <option value="T" #if ($!subInformasi.flagAktif == 'T') selected #end>TIDAK AKTIF</option>
</select>