function validate() {

	var err_count = 0;

	var kadarSewa = $('kadarSewa').value;
	if ( kadarSewa == "" ) {$('err_kadarSewa').innerHTML = "Kadar sewa swasta adalah mandatori!"; err_count++;  }
	
	var kadarSewaAwam = $('kadarSewaAwam').value;
	if ( kadarSewaAwam == "" ) {$('err_kadarSewaAwam').innerHTML = "Kadar sewa awam adalah mandatori!"; err_count++;  }
	
	var masaMula = $('waktuBuka').value;
	if ( masaMula == "" ) {$('err_masaMula').innerHTML = "Masa mula adalah mandatori!"; err_count++;  }
	
	var masaTamat = $('waktuTutup').value;
	if ( masaTamat == "" ) {$('err_masaTamat').innerHTML = "Masa tamat adalah mandatori!"; err_count++;  }
	
	if (err_count == 0) {
		if ( !window.confirm("Anda pasti?") ){
			return;
		}
	}
	
	return err_count == 0;
}

