<div id="content-format"><pinLeft></pinLeft><pinRight></pinRight>
  <header>HUBUNGI KAMI 11</header>
  <fieldset>
      <section>
    	<table cellpadding="0" cellspacing="0" border="0" style="border: 1px dotted #104C83; margin: 0 auto; height: 490px; width: 500px; color: #000; background: #fff url('../images/menu/contact-us.jpg') no-repeat bottom right; background-size: 300px 125px; padding-top: 15px">
    		<tr>
    			<td valign="top" width="50px">
    				&nbsp;
    			</td>	
    			<td valign="top" style="font-size: 14px;">
    				<div class="divBoxhHubungiKami">

    					<div><i class="icon-location"></i><span class="spanPaddingLeft">$hubungiKami.bahagian</span></div>			
						<div class="paddingLeft">$hubungiKami.jabatan</div>	
						<div class="paddingLeft">$hubungiKami.alamat1</div>	
						<div class="paddingLeft">$hubungiKami.alamat2</div>	
						<div class="paddingLeft">$hubungiKami.alamat3</div>	
						<div class="paddingLeft">$hubungiKami.poskod $hubungiKami.bandar.keterangan</div>
						<br/>
						<div><i class="icon-phone"></i><span class="spanPaddingLeft">$hubungiKami.noTelefon</span></div>
						<div><i class="icon-printer"></i><span class="spanPaddingLeft">$hubungiKami.noFaks</span></div>	
						<div><i class="icon-mail"></i><span class="spanPaddingLeft">Emel : $hubungiKami.emel</span></div>

						<br/><br/><br/>
						
						#foreach($hubungiKami in $listHubungiKami)
						<div><i class="icon-location"></i><span class="spanPaddingLeft">$hubungiKami.bahagian</span></div>			
						<div class="paddingLeft">$hubungiKami.alamat1</div>	
						<div class="paddingLeft">$hubungiKami.alamat2</div>	
						<div class="paddingLeft">$hubungiKami.alamat3</div>	
						<div class="paddingLeft">$hubungiKami.poskod $hubungiKami.bandar.keterangan</div>
						<br/>
						<div><i class="icon-phone"></i><span class="spanPaddingLeft">$hubungiKami.noTelefon</span></div>
						<div><i class="icon-printer"></i><span class="spanPaddingLeft">$hubungiKami.noFaks</span></div>	
						<div><i class="icon-mail"></i><span class="spanPaddingLeft">Emel : $hubungiKami.emel</span></div>
						#end
							
    				</div>
  
    			</td>
    		</tr>
    	</table>
      </section>
  </fieldset>
</div>
