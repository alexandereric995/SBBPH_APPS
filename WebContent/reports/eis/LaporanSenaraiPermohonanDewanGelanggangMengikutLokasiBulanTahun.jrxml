	<parameter name="tarikh_1" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[(!$P{tarikhMula}.trim().equals("") && !$P{tarikhHingga}.trim().equals("") ? "AND A.tarikh_mula BETWEEN str_to_date ('" + $P{tarikhMula} + "','%d-%m-%Y') AND str_to_date ('" + $P{tarikhHingga} + "','%d-%m-%Y')" : "")

]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT C.nama, B.tarikh_transaksi, 
A.tarikh_mula, A.tarikh_tamat, A.masa_mula, A.masa_tamat,
(A.masa_tamat - A.masa_mula) AS totalMasa,
B.debit
FROM util_permohonan A
JOIN util_akaun B ON A.id = B.id_permohonan
JOIN util_gelanggang C ON A.id_gelanggang = C.id
WHERE A.status_bayaran = 'Y'
$P!{tarikh_1}
ORDER BY A.tarikh_mula, A.tarikh_tamat]]></queryString>

