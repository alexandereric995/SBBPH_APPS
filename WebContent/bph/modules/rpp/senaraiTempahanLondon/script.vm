
function doLulusPermohonanList(queryParam) {
	if ( !confirm('Adakah anda pasti ?') ){ 
		return; 
	}else{
		$('_wait').innerHTML='Please wait...';
		doDivAjaxChainCall$formname('divstatus','lulusPermohonan',queryParam+'&flag=LIST','div_list','list_page',queryParam);
	}	
}

