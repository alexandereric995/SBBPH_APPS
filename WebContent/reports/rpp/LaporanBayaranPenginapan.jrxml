	<queryString><![CDATA[SELECT rpp_permohonan.tarikh_bayaran,
       users.user_name,
       users.no_kp,
       rpp_permohonan.kaedah_bayaran,
       rpp_permohonan.jumlah_amaun,
       rpp_permohonan.no_tempahan,
       rpp_peranginan.kod_lokasi,
       rpp_peranginan.id,
       rpp_peranginan.nama_peranginan,
       rpp_peranginan.kod_lokasi
  FROM    (   sbbphdb.rpp_permohonan rpp_permohonan
           JOIN
              sbbphdb.rpp_peranginan rpp_peranginan
           ON (rpp_permohonan.id_peranginan = rpp_peranginan.id))
       JOIN
          sbbphdb.users users
       ON (rpp_permohonan.id_pemohon = users.user_login)
$P!{tarikh_1}
$P!{lokasi_1}]]></queryString>

