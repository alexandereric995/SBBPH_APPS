<fieldset>
<legend class="legend">MAKLUMAT REKOD</legend>
<table width="100%" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <td width="50%" valign="top"><table cellpadding="3" cellspacing="0" border="0" width="100%" style="padding: 0px 25px;">
        <tr>
          <td style="width:150px"></td>
          <td style="width:10px"></td>
          <td style="width:150px"></td>
          <td style="width:10px"></td>
        </tr>
		  
		  ##currency_required("kadarSewa" "Sewaan Swasta (RM)" "$!r.kadarSewa")
		  <tr>
			<td class="label" valign="top">Sewaan Swasta (RM) &nbsp;<span style="color:#FF2A00">*</span></td>
			<td class="label" valign="top">:</td>
			<td valign="top">
			<input class="currency" type="text" id="kadarSewa" name="kadarSewa" value="$!r.kadarSewa" onKeyUp=" $('err_kadarSewa').innerHTML=''; at(this, event);">
			<div id="err_kadarSewa" style="color:#CC0000;font-weight:bold;border:2px #000"></div>
			</td>
			<td style="color:#FF2A00"><b>/jam</b></td>
		</tr>
		  ##currency_required("kadarSewaAwam" "Sewaan Awam (RM)" "$!r.kadarSewaAwam")
		  <tr>
			<td class="label" valign="top">Sewaan Awam (RM) &nbsp;<span style="color:#FF2A00">*</span></td>
			<td class="label" valign="top">:</td>
			<td valign="top">
			<input class="currency" type="text" id="kadarSewaAwam" name="kadarSewaAwam" value="$!r.kadarSewaAwam" onKeyUp=" $('err_kadarSewaAwam').innerHTML=''; at(this, event);">
			<div id="err_kadarSewaAwam" style="color:#CC0000;font-weight:bold;border:2px #000"></div>
			</td>
			<td style="color:#FF2A00"><b>/hari</b></td>
		</tr>
		  #textarea("catatan" "Catatan" "$!r.catatan")
		  	  
		 
      </table></td>
    <td width="50%" valign="top"><table cellpadding="3" cellspacing="0" border="0" width="100%" style="padding: 0px 25px;">
        <tr>
          <td style="width:150px"></td>
          <td style="width:10px"></td>
          <td></td>
        </tr>
        <tr>
          <td class="label" valign="top">Masa Mula &nbsp;<span style="color:#FF2A00">*</span></td>
          <td valign="top">:</td>
          <td><div id="selectMasaMula">
            #parse("$path/selectMasaMula.vm")
            <div id="err_masaMula" style="color:#CC0000;font-weight:bold;border:2px #000"></td>
         </tr>
        
		 <tr>
          <td class="label" valign="top">Masa Tamat &nbsp;<span style="color:#FF2A00">*</span></td>
          <td valign="top">:</td>
          <td><div id="selectMasaTamat">
            #parse("$path/selectMasaTamat.vm")
            <div id="err_masaTamat" style="color:#CC0000;font-weight:bold;border:2px #000"></td>
        </tr>
         <tr>
          <td class="label" valign="top">Unit Seksyen &nbsp;<span style="color:#FF2A00">*</span></td>
          <td valign="top">:</td>
          <td>#parse("$path/selectSeksyen.vm")
            <div id="err_seksyen" style="color:#CC0000;font-weight:bold;border:2px #000"></td>
        </tr>
      </table></td>
  </tr>
</table>
</fieldset>



<script>
  _jq(function($) {
      $('.currency').autoNumeric('init'); 
 	  $('.number').autoNumeric('init', {aSep: '', vMin: '0', vMax: '999999999999999', lZero: 'keep'} );
 	  $('.postcode').autoNumeric('init', {aSep: '', vMin: '00000', vMax: '99999', lZero: 'keep'} );
 	  $('.mykad').autoNumeric('init', {aSep: '', vMin: '000000000000', vMax: '999999999999', lZero: 'allow'} );
  });
</script>