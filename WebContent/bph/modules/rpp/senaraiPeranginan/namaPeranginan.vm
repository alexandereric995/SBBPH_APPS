#if ($command == 'selectJenisPeranginan')
 	<input class="texts" type="text" id="namaPeranginan" name="namaPeranginan" value="$namaPeranginan" onKeyUp="at(this, event)" onBlur="this.value=this.value.toUpperCase();">
#else
	<input class="texts" type="text" id="namaPeranginan" name="namaPeranginan" value="$!r.namaPeranginan" onKeyUp="at(this, event)" onBlur="this.value=this.value.toUpperCase();">
#end