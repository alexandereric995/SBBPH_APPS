	dev_hakmilik.no_hakmilik,
	dev_hakmilik.no_lot,
      	dev_hakmilik.lokasi
  FROM  ( ( sbbphdb.dev_hakmilik dev_hakmilik
    LEFT JOIN
     sbbphdb.ruj_negeri ruj_negeri
      ON (dev_hakmilik.id_negeri = ruj_negeri.id))
    LEFT JOIN
     sbbphdb.ruj_mukim ruj_mukim
      ON (dev_hakmilik.id_mukim = ruj_mukim.id))
    LEFT JOIN
     sbbphdb.ruj_daerah ruj_daerah
      ON (dev_hakmilik.id_daerah = ruj_daerah.id)
$P!{param}]]></queryString>

