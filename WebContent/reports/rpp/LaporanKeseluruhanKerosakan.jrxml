	<queryString><![CDATA[select a.id, b.keterangan aduankerosakan, b.tarikh_aduan, b.ulasan_hq ulasanaduan, c.user_name, d.keterangan, e.nama_peranginan
from rpp_permohonan a
join rpp_aduan_kerosakan b on a.id = b.id_permohonan
join users c on a.id_pemohon = c.user_login
join ruj_jenis_unit_rpp d on a.id_jenis_unit_rpp = d.id
join rpp_peranginan e on d.id_peranginan = e.id
where a.id is not null
$P!{tarikh_1}
$P!{idJenisUnit_1}
$P!{idPeranginan_1}
$P!{findJenisPeranginan_1}]]></queryString>

