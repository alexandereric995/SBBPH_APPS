<select id="idJenisSewa" name="idJenisSewa" style="width:100%" onChange="doDivAjaxCall$formname('divPerjanjian','getMaklumatPerjanjian','');$('err_idJenisSewa').innerHTML=''; at(this, event);">
    <option value="">SILA PILIH</option>
    <option #if ( "H" ==  $!perjanjian.idJenisSewa ) selected #end value="H">HARIAN</option>
    <option #if ( "B" ==  $!perjanjian.idJenisSewa ) selected #end value="B">BULANAN</option>
    <option #if ( "T" ==  $!perjanjian.idJenisSewa ) selected #end value="T">TAHUNAN</option>
</select>
