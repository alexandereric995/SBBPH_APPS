<fieldset>
<legend class="legend">LAPORAN SENARAI RESIT</legend>
<table width="100%">
<tr>
    <td style="width:150px"></td>
    <td style="width:10px"></td>
    <td></td>
</tr>
<td class="label" valign="top">Bulan &nbsp;<span style="color:#FF2A00">*</span></td>
                    	<td class="label" valign="top">:</td>
                    	<td valign="top">
                    		<div>
                        		<input type="text" id="bulan" name="bulan" value="" onclick="$('err_bulan').innerHTML = ''">
                    			<script>
                                	_jq("#bulan").Zebra_DatePicker({ 
                        				view: 'month',
										format: 'm'
                                	});
                    				_jq(".Zebra_DatePicker").css("zIndex", 2147483647);
                        		</script>
                    		</div>
                    		<div id="err_bulan" style="color:#CC0000;font-weight:bold;border:2px #000"></div>
                    	</td>
                    </tr>
                    <tr>
                    	<td class="label" valign="top">Tahun &nbsp;<span style="color:#FF2A00">*</span></td>
                    	<td class="label" valign="top">:</td>
                    	<td valign="top">
                    		<div>
                        		<input type="text" id="tahun" name="tahun" value="" onclick="$('err_tahun').innerHTML = ''">
                    			<script>
                                	_jq("#tahun").Zebra_DatePicker({ 
                        				view: 'year',
										format: 'Y'
                                	});
                    				_jq(".Zebra_DatePicker").css("zIndex", 2147483647);
                        		</script>
                    		</div>
                    		<div id="err_tahun" style="color:#CC0000;font-weight:bold;border:2px #000"></div>
                    	</td>
                    </tr>
                      </tr>
  <tr>
    <td class="label">Kod Pusat Terima &nbsp;<span style="color:#FF2A00">*</span></td>
    <td>:</td>
    <td>
    	<span id="listPusatTerima">
        #parse("$path/selectPusatTerima.vm")
		</span>
    </td>
  </tr>
<tr>
	<td></td>
	<td class="label"></td>
	<td><input type="button" name="cmdCetak" id="cmdCetak" value="Cetak" onclick="javascript:doCetakLaporanSenaraiResit()"/><input type="reset" value="Kosongkan" /><input type="button" name="cmdKembali" id="cmdKembali" value="Kembali" onclick="javascript:doKembali()"/></td>
</tr>
</table>
</fieldset>

<script>
  _jq(function($) {
      $('.currency').autoNumeric('init'); 
 	  $('.number').autoNumeric('init', {aSep: '', vMin: '0', vMax: '999999999999999', lZero: 'keep'} );
 	  $('.postcode').autoNumeric('init', {aSep: '', vMin: '00000', vMax: '99999', lZero: 'keep'} );
 	  $('.mykad').autoNumeric('init', {aSep: '', vMin: '000000000000', vMax: '999999999999', lZero: 'allow'} );
  });
</script>