function validate() {
	var err_count = 0;
	
	var tarikhPermohonan = $('tarikhPermohonan').value;
	if ( tarikhPermohonan == "" ) {$('err_tarikhPermohonan').innerHTML = "** mandatori!"; err_count++;  }
	
	var noRujukanPermohonan = $('noRujukanPermohonan').value;
	if ( noRujukanPermohonan == "" ) {$('err_noRujukanPermohonan').innerHTML = "** mandatori!"; err_count++;  }
	
	var tarikhTerimaPermohonan = $('tarikhTerimaPermohonan').value;
	if ( tarikhTerimaPermohonan == "" ) {$('err_tarikhTerimaPermohonan').innerHTML = "** mandatori!"; err_count++;  }
	
	var noPengenalan = $('noPengenalan').value;
	if ( noPengenalan == "" ) {$('err_noPengenalan').innerHTML = "** mandatori!"; err_count++;  }
	
	var namaPemohon = $('namaPemohon').value;
	if ( namaPemohon == "" ) {$('err_namaPemohon').innerHTML = "** mandatori!"; err_count++;  }
	
	var alamat1Pemohon = $('alamat1Pemohon').value;
	if ( alamat1Pemohon == "" ) {$('err_alamat1Pemohon').innerHTML = "** mandatori!"; err_count++;  }
	
	var poskodPemohon = $('poskodPemohon').value;
	if ( poskodPemohon == "" ) {$('err_poskodPemohon').innerHTML = "** mandatori!"; err_count++;  }
	
	var idNegeriPemohon = $('idNegeriPemohon').value;
	if ( idNegeriPemohon == "" ) {$('err_idNegeriPemohon').innerHTML = "** mandatori!"; err_count++;  }
	
	var idBandarPemohon = $('idBandarPemohon').value;
	if ( idBandarPemohon == "" ) {$('err_idBandarPemohon').innerHTML = "** mandatori!"; err_count++;  }
	
	var noTelefonBimbit = $('noTelefonBimbit').value;
	if ( noTelefonBimbit == "" ) {$('err_noTelefonBimbit').innerHTML = "** mandatori!"; err_count++;  }
	
	var namaAgensi = $('namaAgensi').value;
	if ( namaAgensi == "" ) {$('err_namaAgensi').innerHTML = "** mandatori!"; err_count++;  }
	
	var alamat1Agensi = $('alamat1Agensi').value;
	if ( alamat1Agensi == "" ) {$('err_alamat1Agensi').innerHTML = "** mandatori!"; err_count++;  }
	
	var poskodAgensi = $('poskodAgensi').value;
	if ( poskodAgensi == "" ) {$('err_poskodAgensi').innerHTML = "** mandatori!"; err_count++;  }
	
	var idNegeriAgensi = $('idNegeriAgensi').value;
	if ( idNegeriAgensi == "" ) {$('err_idNegeriAgensi').innerHTML = "** mandatori!"; err_count++;  }
	
	var idBandarAgensi = $('idBandarAgensi').value;
	if ( idBandarAgensi == "" ) {$('err_idBandarAgensi').innerHTML = "** mandatori!"; err_count++;  }
	
	if (err_count == 0) {
		if ( !window.confirm("Anda pasti?") ){
			return;
		}
	}
	
	return err_count == 0;
}