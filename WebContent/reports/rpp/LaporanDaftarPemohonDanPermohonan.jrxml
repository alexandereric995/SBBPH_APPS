	<queryString><![CDATA[SELECT B.user_name, A.status_bayaran, A.no_tempahan, A.tarikh_masuk_rpp, A.tarikh_keluar_rpp, A.tarikh_permohonan, A.jenis_permohonan, A.gred_jawatan,
ruj_status.keterangan status, C.id_jenis_peranginan, C.nama_peranginan
FROM ((rpp_permohonan A
JOIN rpp_peranginan C ON (A.id_peranginan = C.id))
JOIN users B ON (A.id_pemohon = B.user_login))
JOIN ruj_status ON (A.id_status = ruj_status.id)
WHERE A.id is not null
AND A.jenis_permohonan = '$P!{jenis_permohonan}'
AND C.id_jenis_peranginan = '$P!{findJenisPeranginan}'
AND A.id_peranginan = '$P!{findPeranginan}']]></queryString>

