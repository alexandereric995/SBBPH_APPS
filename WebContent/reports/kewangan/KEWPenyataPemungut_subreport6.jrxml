	<parameter name="kodPusat" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT A.no_resit, A.tarikh_bayaran, C.no_cek, C.tempat, C.amaun_bayaran, D.keterangan, SUM(C.amaun_bayaran) AS TOTAL 
FROM kew_bayaran_resit A
JOIN kew_resit_kaedah_bayaran C ON A.id = C.id_bayaran_resit
LEFT JOIN ruj_bank D ON C.id_bank = D.id
JOIN ruj_kod_juruwang F ON A.id_juruwang = F.id
WHERE DATE_FORMAT(A.tarikh_bayaran,'%d-%m-%Y') = $P!{tarikhPenyataPemungut} 
AND A.id_juruwang is not null
AND C.id_mod_bayaran = '$P!{kaedahBayaran}'
AND a.id_kod_pusat_terima = '$P!{kodPusat}'
AND (A.flag_void is null or A.flag_void = 'T')
AND C.id_mod_bayaran != 'T'
GROUP BY A.no_resit, A.tarikh_bayaran, C.no_cek, C.tempat, C.amaun_bayaran, D.keterangan]]></queryString>

