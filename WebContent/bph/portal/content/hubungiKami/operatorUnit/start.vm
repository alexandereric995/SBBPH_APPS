<div id="content-format"><pinLeft></pinLeft><pinRight></pinRight>
  <header>Operator Unit</header>
  <section>
  	<table cellpadding="0" cellspacing="2" border="0" width="100%" class="table table-bordered">
  		<thead>
			<tr style="border:1px solid #000; font-size:17px;">
				<td>Bil</td>			
				<td>Unit</td>
				<td>Nama</td>
				<td>Emel</td>
				<td>No. Tel</td>
			</tr>
		</thead>
		#set($bil = 0)
		#foreach($operator in $operatorUnit)
			#set ($bil = $bil + 1)
			<tr>
				<td>$bil</td>					
				<td>$!operator.unit</td>
				<td>$!operator.nama</td>
				<td>$!operator.emel</td>
				<td>$!operator.noTelefon</td>
			</tr>
		#end
  	</table>
