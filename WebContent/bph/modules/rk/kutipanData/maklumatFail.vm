<fieldset>
<legend class="legend">Maklumat Fail</legend>
<table width="100%" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <td width="50%" valign="top"><table cellpadding="3" cellspacing="0" border="0" width="100%" style="padding: 0px 25px;">
        <tr>
          <td style="width:34%"></td>
          <td style="width:1%"></td>
          <td style="width:65%"></td>
        </tr>
        <tr>
          <td class="label" valign="top">No. Fail &nbsp;<span style="color:#FF2A00">*</span></td>
          <td valign="top">:</td>
          <td> <input type="text" id="noFail" name="noFail" value="$!fail.noFail" onChange="doDivAjaxCall3$formname('divFail','doSemakNoFail','')" onKeyUp="$('err_noFail').innerHTML='';$('err_noFailWujud').innerHTML=''; at(this, event)" onBlur="this.value=this.value.toUpperCase();">
            <div id="err_noFail" style="color:#CC0000;font-weight:bold;border:2px #000"></div>
            #if ( $existNoFail )
            <div id="err_noFailWujud" style="color:#CC0000;font-weight:bold;border:2px #000">NO. FAIL TELAH WUJUD</div>
            #end
            </td>
        </tr>         
      </table></td>
    <td width="50%" valign="top"><table cellpadding="3" cellspacing="0" border="0" width="100%" style="padding: 0px 25px;">
        <tr>
          <td style="width:34%"></td>
          <td style="width:1%"></td>
          <td style="width:65%"></td>
        </tr>
      </table></td>
  </tr>
</table>
</fieldset>