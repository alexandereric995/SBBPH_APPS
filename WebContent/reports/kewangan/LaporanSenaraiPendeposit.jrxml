	<queryString><![CDATA[SELECT c.user_name,c.no_kp,a.tarikh_bayaran tarikh_resit, a.no_resit, a.jumlah_deposit, a.id_kod_hasil
FROM kew_deposit a
LEFT JOIN kew_tuntutan_deposit b ON a.id_tuntutan_deposit = b.id
LEFT JOIN users c ON a.id_pendeposit = c.user_login
WHERE a.id_kod_hasil = $P!{kodHasil}
AND a.id_jenis_bayaran = $P!{idJenisBayaran}
and ifnull(a.flag_warta,'T') <> 'Y'
and ifnull(a.flag_pulang_deposit,'T') = 'T' 
and ifnull(a.flag_bayar,'T') = 'Y'
$P!{tarikhHingga_1}
order by a.tarikh_bayaran desc]]></queryString>

