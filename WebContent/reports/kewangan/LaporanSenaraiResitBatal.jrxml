	<queryString><![CDATA[SELECT tarikh_bayaran,
       no_resit,
       amaun_void,
       tarikh_batal_resit,
       catatan_batal_resit,
       id_juruwang,
       kod_juruwang,
       flag_void,
       nama_pembayar,
       no_pengenalan_pembayar
FROM kew_bayaran_resit
WHERE     flag_void = 'Y'
      AND id_kod_pusat_terima = '$P!{kodPusat}'
      AND tarikh_batal_resit BETWEEN str_to_date('$P!{tarikhMula}', '%d-%m-%Y')
                                 AND str_to_date('$P!{tarikhHingga}', '%d-%m-%Y')
ORDER BY masa_resit]]></queryString>

