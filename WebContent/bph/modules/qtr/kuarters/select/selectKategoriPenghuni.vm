	<option #if ( $!r.kategoriPenghuni == 'BERKELUARGA' ) selected #end value="BERKELUARGA">BERKELUARGA</option>
	<option #if ( $!r.kategoriPenghuni == 'BUJANG' ) selected #end value="BUJANG">BUJANG</option>
</select>
