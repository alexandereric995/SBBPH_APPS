	</parameter>
	<parameter name="TarikhMula" isForPrompting="true" class="java.lang.String"/>
	<parameter name="TarikhAkhir" isForPrompting="true" class="java.lang.String"/>
	<parameter name="tarikh_1" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[(!$P{TarikhMula}.trim().equals("") && !$P{TarikhAkhir}.trim().equals("") ? "WHERE rk_permohonan.tarikh_permohonan BETWEEN str_to_date ('" + $P{TarikhMula} + "','%d-%m-%Y') AND str_to_date ('" + $P{TarikhAkhir} + "','%d-%m-%Y')" : "")

]]></defaultValueExpression>
	</parameter>
	<parameter name="TempohPerjanjian" isForPrompting="true" class="java.lang.String"/>
