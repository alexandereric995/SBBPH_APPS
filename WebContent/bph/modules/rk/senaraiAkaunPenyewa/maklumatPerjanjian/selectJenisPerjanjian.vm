<select id="idJenisPerjanjian" name="idJenisPerjanjian" style="width:100%" onChange="$('err_idJenisPerjanjian').innerHTML=''; at(this, event);doDivAjaxCall3$formname('popupPerjanjian','doChangeJenisPerjanjian','')">
   <option value="">SILA PILIH</option>
    <option #if ( "2" ==  $!perjanjian.idJenisPerjanjian ) selected #end value="2">PERLANJUTAN</option>
    <option #if ( "3" ==  $!perjanjian.idJenisPerjanjian ) selected #end value="3">PENGURANGAN / PENAMBAHAN KADAR SEWA</option>
    <option #if ( "4" ==  $!perjanjian.idJenisPerjanjian ) selected #end value="4">PENGECUALIAN KADAR SEWA</option>
	<option #if ( "5" ==  $!perjanjian.idJenisPerjanjian ) selected #end value="5">PENGURANGAN / PENAMBAHAN CAJ IWK</option>
	<option #if ( "6" ==  $!perjanjian.idJenisPerjanjian ) selected #end value="6">PENGECUALIAN CAJ IWK</option>	   
</select>