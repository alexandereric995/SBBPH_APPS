JOIN users b on a.id_pendeposit = b.user_login
JOIN kua_penghuni c on a.id_pendeposit = c.id_pemohon
WHERE a.id_kod_hasil = '72310'
and c.tarikh_keluar_kuarters in (select tarikh_keluar_kuarters
from kua_penghuni
where DATE_FORMAT(DATE_ADD(tarikh_keluar_kuarters,INTERVAL 1 YEAR), '%d/%m/%y') <= DATE_FORMAT(current_date(),'%d/%m/%y'))
and a.flag_warta = 'T']]></queryString>

