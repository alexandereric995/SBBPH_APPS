	<parameter name="zon_1" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[(!$P{zon}.equals("") ? "AND UPPER(ruj_negeri.id_zon) like '%" + $P{zon} + "%'" : "")
]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT ruj_negeri.keterangan negeri,
       dev_ruang.nama_ruang,
       dev_ruang.luas_ruang,
       dev_ruang.tinggi_siling,
       dev_bangunan.nama_bangunan,
       ruj_zon.keterangan zon,
       dev_aras.nama_aras,
       bgs_ruang.kadar_sewa,
       bgs_ruang.flag_kekosongan status
  FROM    (   (   (   (   (   (   (   sbbphdb.ruj_mukim ruj_mukim
                                   JOIN
                                      sbbphdb.ruj_daerah ruj_daerah
                                   ON (ruj_mukim.id_daerah = ruj_daerah.id))
                               JOIN
                                  sbbphdb.dev_premis dev_premis
                               ON (dev_premis.id_mukim = ruj_mukim.id))
                           JOIN
                              sbbphdb.dev_bangunan dev_bangunan
                           ON (dev_bangunan.id_premis = dev_premis.id))
                       JOIN
                          sbbphdb.dev_aras dev_aras
                       ON (dev_aras.id_bangunan = dev_bangunan.id))
                   JOIN
                      sbbphdb.dev_ruang dev_ruang
                   ON (dev_ruang.id_aras = dev_aras.id))
               JOIN
                  sbbphdb.bgs_ruang bgs_ruang
               ON (bgs_ruang.id_ruang = dev_ruang.id))
           JOIN
              sbbphdb.ruj_negeri ruj_negeri
           ON (ruj_daerah.id_negeri = ruj_negeri.id))
       JOIN
          sbbphdb.ruj_zon ruj_zon
       ON (ruj_negeri.id_zon = ruj_zon.id)
$P!{status_1}
$P!{zon_1}]]></queryString>

