	<parameter name="findTarikhBayaran" isForPrompting="true" class="java.lang.String"/>
	<parameter name="findTarikhBayaranHingga" isForPrompting="true" class="java.lang.String"/>
	<parameter name="kodPusat" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT kew_bayaran_resit.no_resit, users.user_name, users.user_login,
ruj_kod_hasil.kod, kew_bayaran_resit.jumlah_amaun_bayaran,
kew_bayaran_resit.tarikh_bayaran
FROM kew_bayaran_resit
LEFT JOIN users on kew_bayaran_resit.id_pembayar = users.user_login
LEFT JOIN kew_resit_senarai_invois on kew_bayaran_resit.id = kew_resit_senarai_invois.id_bayaran_resit
LEFT OUTER JOIN kew_invois on kew_resit_senarai_invois.id_invois = kew_invois.id
LEFT JOIN ruj_kod_hasil on kew_invois.id_kod_hasil = ruj_kod_hasil.id
WHERE kew_bayaran_resit.id is not null
AND kew_bayaran_resit.flag_jenis_bayaran = 'MANUAL'
AND kew_bayaran_resit.id_kod_pusat_terima = '$P!{kodPusat}'
AND kew_bayaran_resit.tarikh_bayaran BETWEEN str_to_date ('$P!{findTarikhBayaran}','%d-%m-%Y') 
AND str_to_date ('$P!{findTarikhBayaranHingga}','%d-%m-%Y')
AND ruj_kod_hasil.id = '$P!{findKodHasil}'
UNION 
SELECT kew_bayaran_resit.no_resit, users.user_name, users.user_login,
ruj_kod_hasil.kod, kew_bayaran_resit.jumlah_amaun_bayaran,
kew_bayaran_resit.tarikh_bayaran
FROM kew_bayaran_resit
LEFT JOIN users on kew_bayaran_resit.id_pembayar = users.user_login
LEFT JOIN kew_resit_senarai_invois on kew_bayaran_resit.id = kew_resit_senarai_invois.id_bayaran_resit
LEFT OUTER JOIN kew_deposit on kew_resit_senarai_invois.id_deposit = kew_deposit.id
LEFT JOIN ruj_kod_hasil on kew_deposit.id_kod_hasil = ruj_kod_hasil.id
WHERE kew_bayaran_resit.id is not null
AND kew_bayaran_resit.flag_jenis_bayaran = 'MANUAL'
AND kew_bayaran_resit.id_kod_pusat_terima = '$P!{kodPusat}'
AND kew_bayaran_resit.tarikh_bayaran BETWEEN str_to_date ('$P!{findTarikhBayaran}','%d-%m-%Y') 
AND str_to_date ('$P!{findTarikhBayaranHingga}','%d-%m-%Y')
AND ruj_kod_hasil.id = '$P!{findKodHasil}'
ORDER BY tarikh_bayaran,no_resit]]></queryString>

