	<queryString><![CDATA[SELECT rpp_peranginan.nama_peranginan,
       rpp_permohonan.bil_dewasa,
       rpp_permohonan.bil_kanak_kanak,
       COUNT(rpp_permohonan.id) total,       
       rpp_permohonan.tarikh_permohonan       
  FROM    (   sbbphdb.rpp_permohonan rpp_permohonan
           JOIN
              sbbphdb.ruj_status ruj_status
           ON (rpp_permohonan.id_status = ruj_status.id))
       JOIN
          sbbphdb.rpp_peranginan rpp_peranginan
       ON (rpp_peranginan.id = rpp_permohonan.id_peranginan)
