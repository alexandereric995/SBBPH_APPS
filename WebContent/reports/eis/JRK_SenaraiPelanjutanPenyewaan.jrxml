	</parameter>
	<queryString><![CDATA[SELECT rk_fail.no_fail, rk_permohonan.id, rk_permohonan.tarikh_permohonan, rk_permohonan.no_rujukan_surat, rk_pemohon.nama,
rk_ruang_komersil.nama_ruang, rk_ruang_komersil.lokasi, ruj_jenis_kegunaan_ruang.keterangan jenis_kegunaan_ruang,
rk_perjanjian.tempoh, rk_perjanjian.tarikh_tamat, rk_perjanjian.kadar_sewa, rk_perjanjian.no_rujukan, rk_perjanjian.flag_aktif
FROM rk_ruang_komersil
JOIN ruj_jenis_kegunaan_ruang ON rk_ruang_komersil.id_jenis_kegunaan_ruang = ruj_jenis_kegunaan_ruang.id
JOIN rk_fail ON rk_fail.id_ruang = rk_ruang_komersil.id
JOIN rk_permohonan ON rk_permohonan.id_fail = rk_fail.id
JOIN rk_perjanjian ON rk_perjanjian.id_permohonan = rk_permohonan.id
JOIN rk_pemohon ON rk_fail.id_pemohon = rk_pemohon.id
$P!{tarikh_1}
$P!{idLokasi_1}
$P!{idKegunaanRuang_1}
$P!{TempohPerjanjian_1}
$P!{idStatus_1}]]></queryString>

