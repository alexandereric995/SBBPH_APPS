	<queryString><![CDATA[SELECT users.user_name,
       users.no_kp,
       kua_penghuni.id_pemohon,
       kua_kuarters.no_unit,
       ruj_lokasi_kuarters.keterangan,
       ruj_kelas_kuarters.id,
       kua_permohonan.id idPermohonan
FROM kua_penghuni kua_penghuni
LEFT OUTER JOIN users users ON (kua_penghuni.id_pemohon = users.user_login)
LEFT OUTER JOIN kua_permohonan kua_permohonan ON (kua_penghuni.id_permohonan = kua_permohonan.id)
LEFT OUTER JOIN kua_kuarters kua_kuarters ON (kua_penghuni.id_kuarters = kua_kuarters.id)
LEFT OUTER JOIN ruj_lokasi_kuarters ruj_lokasi_kuarters ON (kua_kuarters.id_lokasi_kuarters = ruj_lokasi_kuarters.id)
LEFT OUTER JOIN ruj_kelas_kuarters ruj_kelas_kuarters ON (kua_kuarters.id_kelas_kuarters = ruj_kelas_kuarters.id)
WHERE kua_penghuni.id is not null
$P!{findNoPermohonan_cond}
$P!{findNoKP_cond}]]></queryString>

