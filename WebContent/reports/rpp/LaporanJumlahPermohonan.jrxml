	</parameter>
	<queryString><![CDATA[SELECT rpp_peranginan.nama_peranginan, ruj_jenis_unit_rpp.keterangan jenisUnit, COUNT(rpp_permohonan.id) total,     
ruj_status.id status, rpp_permohonan.jumlah_amaun, rpp_permohonan.tarikh_permohonan, rpp_permohonan.id_jenis_unit_rpp
FROM ( rpp_permohonan
JOIN ruj_status ON (rpp_permohonan.id_status = ruj_status.id))
JOIN rpp_peranginan ON (rpp_peranginan.id = rpp_permohonan.id_peranginan)
JOIN ruj_jenis_unit_rpp  ON (rpp_permohonan.id_jenis_unit_rpp = ruj_jenis_unit_rpp.id)
WHERE rpp_permohonan.id_status in ('1425259713415','1425259713418','1435093978588')
GROUP BY ruj_jenis_unit_rpp.id, ruj_status.id, rpp_peranginan.nama_peranginan
$P!{tarikh_1}
$P!{idPeranginan_1}]]></queryString>

