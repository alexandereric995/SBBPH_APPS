			"initComplete": function () {
                this.api().columns().every( function () {
                    var column = this;
                    var select = _jq('<select><option value=""></option></select>')
                        .appendTo( _jq(column.footer()).empty() )
                        .on( 'change', function () {
                            var val = _jq.fn.dataTable.util.escapeRegex(
                                _jq(this).val()
                            );
     
                            column
                                .search( val ? '^'+val+'$' : '', true, false )
                                .draw();
                        } );
     
                    column.data().unique().sort().each( function ( d, j ) {
                        select.append( '<option value="'+d+'">'+d+'</option>' )
                    } );
